<div class='gallery'>
    <div class="gallery-item upload-link" style="background: rgb(194, 37, 37);">
        <div><%= link_to "Upload a new image", new_image_path %></div>
    </div>
    <% @images.each do |image| %>
        <% if image.imageFile.attached? %>
        
        <div class="gallery-item" style="background-image: url(<%= url_for(image.imageFile)%>);">
            <div><%= link_to image.title, url_for(image.imageFile) %></div>
        </div>
        
        <% end %>
    <% end %>
</div>